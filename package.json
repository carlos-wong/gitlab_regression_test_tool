{
  "homepage": "./",
  "productName": "leju-test-tool",
  "main": "electron.js",
  "name": "leju-test-tool",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "request": "^2.88.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf ./public/* && cp ./src/index.html ./public/ && react-scripts build && cp -rf build ./public/ && webpack-cli --config webpack.config.js && cp package.json ./public/package.json && cd ./public/ && rm -rf node_modules && yarn install --prod",
    "test": "react-scripts test --env=jsdom",
    "jest": "jest",
    "webpackelectron": "webpack",
    "eject": "react-scripts eject",
    "electron-build-mac": " npm run build && electron-packager ./public --overwrite --platform darwin",
    "electron-build-win32": " npm run build && electron-packager ./public --overwrite --platform win32",
    "electron-dev": "concurrently \"PORT=3001 REACTDEV=true BROWSER=none yarn start\" \"wait-on http://localhost:3001 && electron ./src/electron.js\"",
    "clean": "rm -rf leju-test-tool-darwin-x64* && rm -rf leju-test-tool-win32-x64"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "antd": "^3.4.2",
    "aphrodite": "^2.2.0",
    "async": "^2.6.1",
    "axios": "^0.18.0",
    "babel-jest": "^24.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^3.5.1",
    "electron": "^4.0.3",
    "electron-builder": "^20.10.0",
    "electron-default-menu": "^1.0.1",
    "electron-is-dev": "^0.3.0",
    "electron-packager": "^13.0.1",
    "jest": "^24.0.0",
    "lodash": "^4.17.5",
    "react": "^16.3.2",
    "react-debounce-input": "^3.2.0",
    "react-dom": "^16.3.2",
    "react-flexbox-grid": "^2.0.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3",
    "react-virtual-list": "^2.3.0",
    "redux": "^4.0.1",
    "regenerator-runtime": "^0.13.1",
    "request-promise": "^4.2.2",
    "wait-on": "^2.1.0",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
